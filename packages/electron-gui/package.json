{
  "name": "@google/gemini-cli-electron-gui",
  "version": "0.1.0",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "description": "Electron GUI for Gemini CLI",
  "scripts": {
    "dev": "npm run build --prefix ../core && vite",
    "build": "npm run build --prefix ../core && tsc && vite build",
    "preview": "npm run build --prefix ../core && vite preview",
    "electron:dev": "npm run build --prefix ../core && vite",
    "electron:build": "npm run build --prefix ../core && vite build && electron-builder"
  },
  "dependencies": {
    "@google/gemini-cli": "file:../cli",
    "node-pty": "^1.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "build": {
    "asarUnpack": [
      "**/*.node"
    ]
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.8.3",
    "vite": "^5.2.8",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
